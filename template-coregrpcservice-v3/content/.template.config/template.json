{
  "$schema": "http://json.schemastore.org/template",
  "author": "yaofeng",
  "classifications": [ "Common", "Console" ],
  "identity": "Overt.GrpcTemplateV3.Service",
  "name": "Overt.GrpcTemplateV3.Service",
  "shortName": "OvertGrpcTemplateV3",
  "preferNameDirectory": true,
  "symbols": {
    "namespace": {
      "type": "parameter",
      "isRequired": true,
      "description": "命名空间前缀, .Service为固定后缀 [namespace].Service",
      "defaultValue": "Overt.GrpcTemplate",
      "replaces": "Overt.GrpcTemplate"
    },
    "service": {
      "type": "parameter",
      "isRequired": true,
      "description": "Grpc服务名称 [service]",
      "defaultValue": "TemplateService",
      "replaces": "TemplateService"
    },
    "regname": {
      "type": "parameter",
      "isRequired": true,
      "description": "注册Consul的服务名称 [servicename]",
      "defaultValue": "OvertGrpcTemplateService",
      "replaces": "OvertGrpcTemplateService"
    },
    "port": {
      "type": "parameter",
      "isRequired": true,
      "description": "服务端口号 [serviceport]",
      "defaultValue": "10001",
      "replaces": "10000"
    }
  }
}